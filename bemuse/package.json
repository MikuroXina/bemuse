{
  "name": "bemuse",
  "version": "54.0.1",
  "description": "BEAT☆MUSIC☆SEQUENCE, a web-based music game of the future",
  "private": true,
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "wireit",
    "dev": "vite dev",
    "typecheck": "tsc",
    "pre-deploy": "wireit",
    "prod-build": "wireit",
    "prod-start": "wireit",
    "start": "wireit",
    "test": "wireit",
    "coverage": "wireit",
    "lint": "eslint ."
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "dependencies": [
        "../packages/bemuse-indexer:build",
        "../packages/bemuse-notechart:build",
        "../packages/monetizer:build",
        "../packages/bms:build",
        "../packages/bmson:build"
      ]
    },
    "pre-deploy": {
      "command": "gulp pre-deploy",
      "dependencies": [
        "build",
        "../website:build"
      ]
    },
    "prod-build": {
      "command": "gulp build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prod-start": {
      "command": "gulp server",
      "env": {
        "NODE_ENV": "production",
        "HOT": "true"
      }
    },
    "start": {
      "command": "gulp server",
      "env": {
        "HOT": "true"
      }
    },
    "test": {
      "command": "gulp test",
      "env": {
        "NODE_ENV": "test",
        "BEMUSE_COV": ""
      },
      "dependencies": [
        "build"
      ]
    },
    "coverage": {
      "command": "gulp test",
      "env": {
        "NODE_ENV": "test",
        "BEMUSE_COV": "true"
      }
    }
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "renke"
    }
  },
  "author": "Thai Pangsakulyanont <dtinth@spacet.me> (http://dt.in.th/)",
  "license": "AGPL-1.0",
  "devDependencies": {
    "@types/fastclick": "^1.0.33",
    "@types/invariant": "^2.2.37",
    "@types/jquery": "^3.5.33",
    "@types/lodash": "^4.17.23",
    "@types/markdown-it": "^12.2.3",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^25.3.0",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@types/webpack-env": "^1.18.8",
    "@types/wicg-file-system-access": "^2020.9.8",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.24",
    "body-parser": "^1.20.4",
    "brfs": "^1.6.1",
    "buffer": "^6.0.3",
    "chalk": "^2.4.2",
    "circumstance": "^1.1.1",
    "connect": "^3.7.0",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^6.2.0",
    "eslint": "^9.39.3",
    "eslint-config-bemuse": "workspace:*",
    "events": "^3.3.0",
    "express": "^4.22.1",
    "gauge": "^5.0.0",
    "glob": "^13.0.0",
    "globals": "^17.3.0",
    "gulp": "^5.0.1",
    "js-yaml": "^3.14.2",
    "merge-stream": "^2.0.0",
    "minimatch": "^3.1.3",
    "monetizer": "workspace:*",
    "node-env": "^0.1.6",
    "path-browserify": "^1.0.1",
    "peggy": "^5.0.6",
    "playwright": "^1.58.2",
    "process": "^0.11.10",
    "pug": "^2.0.4",
    "puppeteer": "^24.37.0",
