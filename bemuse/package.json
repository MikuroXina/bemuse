    "timesynchro": "^1.0.1",
    "variance": "0.0.1",
    "whatwg-fetch": "^1.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,scss,css,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}
