{
  "name": "bemuse",
  "version": "54.0.1",
  "description": "BEAT☆MUSIC☆SEQUENCE, a web-based music game of the future",
  "private": true,
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "typecheck": "tsc",
    "pre-deploy": "gulp pre-deploy",
    "prod-build": "wireit",
    "prod-start": "wireit",
    "start": "wireit",
    "test": "wireit",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "wireit": {
    "prod-build": {
      "command": "gulp build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prod-start": {
      "command": "gulp server",
      "env": {
        "NODE_ENV": "production",
        "HOT": "true"
      }
    },
    "start": {
      "command": "gulp server",
      "env": {
        "HOT": "true"
      }
    },
    "test": {
      "command": "gulp test",
      "env": {
        "NODE_ENV": "test",
        "BEMUSE_COV": "true"
      }
    }
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "renke"
    }
  },
  "author": "Thai Pangsakulyanont <dtinth@spacet.me> (http://dt.in.th/)",
  "license": "AGPL-1.0",
