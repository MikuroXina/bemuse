{
  "name": "@mikuroxina/bemuse-indexer",
  "version": "52.0.0",
  "description": "Parse and index BMS notecharts",
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "wireit",
    "lint": "eslint ."
  },
  "wireit": {
    "build": {
      "command": "tsc -b",
      "dependencies": [
        "../bms:build",
        "../bmson:build"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bemusic/indexer.git"
  },
  "author": "Thai Pangsakulyanont <org.yi.dttvb@gmail.com> (http://dt.in.th/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bemusic/indexer/issues"
  },
  "homepage": "https://github.com/bemusic/indexer",
  "dependencies": {
    "bemuse-types": "workspace:*",
    "bms": "workspace:*",
    "bmson": "workspace:*",
    "invariant": "^2.2.4",
    "lodash": "^4.17.11",
    "object-assign": "^4.1.1",
    "p-map": "^4.0.0"
  },
  "devDependencies": {
    "@types/invariant": "^2.2.30",
    "@types/lodash": "^4.14.176",
    "@types/node": "^24.10.13",
    "@types/object-assign": "^4.0.30",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.0.0",
    "eslint-config-bemuse": "workspace:*",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wireit": "^0.14.12"
  }
}
