{
  "name": "@mikuroxina/bemuse-notechart",
  "version": "50.1.2",
  "description": "Bemuse notechart data structure, supports BMS and bmsons.",
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "build": "wireit",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint ."
  },
  "wireit": {
    "build": {
      "command": "tsc -b",
      "dependencies": [
